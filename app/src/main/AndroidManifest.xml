<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.leo.newcity">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".EuropaCity" />
        <activity android:name=".AfricaDetailCity" />
        <activity android:name=".AsiaDetailCity" />
        <activity android:name=".AustraliaDetailCity" />
        <activity android:name=".SouthAmDetailCity" />
        <activity android:name=".NorthAmDetailCity" />
        <activity android:name=".AppInfo"
            android:parentActivityName=".MainActivity" >
            <!-- Parent activity meta-data to support 4.0 and lower -->
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainActivity" />></activity>
    </application>

</manifest>